{
    "plugins": [
        "plugins/markdown",
        "node_modules/better-docs/category"
    ],
    "recurseDepth": 10,
    "source": {
        "include": [ "./src" , "./README.md" ],
        "includePattern": ".+\\.js(doc|x)?$"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "opts": {
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true,
        "template": "./node_modules/better-docs"
    }
}